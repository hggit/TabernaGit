<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:orientation="vertical"
android:layout_width="match_parent"
android:background="#1e1e1d"
android:layout_height="match_parent">
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:weightSum="100"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="40">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="1"
                android:id="@+id/p"
                android:layout_marginTop="30dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="72dp"
                    android:padding="8dp"
                    android:orientation="horizontal"
                    android:layout_gravity="center"
                    android:background="@android:drawable/dialog_holo_light_frame">
                    <ImageButton
                        android:id="@+id/action_voice_btn"
                        style="@style/MSV_ImageButton"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:scaleType="centerCrop"
                        android:background="#f45f"
                        android:onClick="onSearch"
                        android:src="@drawable/ic_action_action_search"
                        android:layout_width="50dp"
                        android:layout_height="match_parent" />
                    <EditText android:id="@+id/search_key"
                        android:layout_width="match_parent"
                        android:layout_height="59dp"
                        android:padding="8dp"
                        android:hint="Search"
                        android:inputType="text"
                        android:background="@android:color/transparent"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="1"
                android:layout_marginTop="24dp"
                android:layout_below="@+id/p"
                android:orientation="vertical">
                <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:id="@+id/table_layout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TableRow
                        android:id="@+id/row_123"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="8dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="122dp"
                            android:layout_weight="1"
                            android:layout_margin="0dp"
                            android:padding="0dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:onClick="Books"
                            android:gravity="center"
                            android:textColor="#fff"
                            android:weightSum="1">
                            <ImageButton
                                android:id="@+id/action_voice_bt7"
                                style="@style/MSV_ImageButton"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:src="@drawable/pooks"
                                android:scaleType="fitXY"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:layout_weight="1.28" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Books"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:textColor="#fbfcf9"
                                android:id="@+id/textView456" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="122dp"
                            android:layout_weight="1"
                            android:layout_margin="0dp"
                            android:padding="0dp"
                            android:clickable="true"
                            android:contextClickable="true"
                            android:onClick="Toys"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:textColor="#fff"
                            android:weightSum="1">
                            <ImageButton
                                android:id="@+id/action_voice_t7"
                                style="@style/MSV_ImageButton"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:src="@drawable/toys"
                                android:layout_width="100dp"
                                android:scaleType="centerCrop"
                                android:layout_height="100dp"
                                android:layout_weight="1.28" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Toys"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:textColor="#fbfcf9"
                                android:id="@+id/textView5" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="122dp"
                            android:layout_weight="1"
                            android:layout_margin="0dp"
                            android:padding="0dp"
                            android:orientation="vertical"
                            android:onClick="Medicine"
                            android:clickable="true"
                            android:gravity="center"
                            android:textColor="#fff"
                            android:weightSum="1">
                            <ImageButton
                                android:id="@+id/action_voice_tn"
                                style="@style/MSV_ImageButton"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:src="@drawable/cloths"
                                android:scaleType="centerCrop"
                                android:layout_width="80dp"
                                android:layout_height="80dp"
                                android:layout_weight="1.28" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Medicine"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:textColor="#fbfcf9"
                                android:id="@+id/textView45" />
                        </LinearLayout>
                    </TableRow>
                    <TableRow
                        android:id="@+id/row_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="122dp"
                            android:layout_weight="1"
                            android:layout_margin="0dp"
                            android:padding="0dp"
                            android:orientation="vertical"
                            android:onClick="DailyUse"
                            android:clickable="true"
                            android:gravity="center"
                            android:textColor="#fff"
                            android:weightSum="1">
                            <ImageButton
                                android:id="@+id/action_voice_bts7"
                                style="@style/MSV_ImageButton"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:src="@drawable/posmetic"
                                android:layout_width="100dp"
                                android:scaleType="centerCrop"
                                android:layout_height="100dp"
                                android:layout_weight="1.28" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Daily Use"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:textColor="#fbfcf9"
                                android:id="@+id/textView457" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="122dp"
                            android:layout_weight="1"
                            android:layout_margin="0dp"
                            android:padding="0dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:onClick="Appliances"
                            android:gravity="center"
                            android:textColor="#fff"
                            android:weightSum="1">
                            <ImageButton
                                android:id="@+id/action_voice"
                                style="@style/MSV_ImageButton"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:src="@drawable/pofa"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:scaleType="centerCrop"
                                android:layout_weight="1.28" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Appliances"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:textColor="#fbfcf9"
                                android:id="@+id/tedxtView" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="122dp"
                            android:layout_weight="1"
                            android:layout_margin="0dp"
                            android:padding="0dp"
                            android:clickable="true"
                            android:orientation="vertical"
                            android:onClick="Gadgets"
                            android:gravity="center"
                            android:textColor="#fff"
                            android:weightSum="1">
                            <ImageButton
                                android:id="@+id/action_voice_tdn"
                                style="@style/MSV_ImageButton"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:src="@drawable/qlec"
                                android:scaleType="centerCrop"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:layout_weight="1.28" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Gadgets"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:textColor="#fbfcf9"
                                android:id="@+id/textView4d5" />
                        </LinearLayout>
                    </TableRow>
                    <TableRow
                        android:id="@+id/row_12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="122dp"
                            android:layout_weight="1"
                            android:layout_margin="0dp"
                            android:padding="0dp"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:textColor="#fff"
                            android:weightSum="1">

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="122dp"
                            android:layout_weight="1"
                            android:layout_margin="0dp"
                            android:padding="0dp"
                            android:orientation="vertical"
                            android:clickable="true"
                            android:onClick="Stationery"
                            android:gravity="center"
                            android:textColor="#fff"
                            android:weightSum="1">
                            <ImageButton
                                android:id="@+id/action_voice_tdeq7"
                                style="@style/MSV_ImageButton"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentRight="true"
                                android:src="@drawable/point"
                                android:layout_width="100dp"
                                android:scaleType="centerCrop"
                                android:layout_height="100dp"
                                android:layout_weight="1.28" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Stationery"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:textColor="#fbfcf9"
                                android:id="@+id/tedxtView5" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="122dp"
                            android:layout_weight="1"
                            android:layout_margin="0dp"
                            android:padding="0dp"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:textColor="#fff"
                            android:weightSum="1">

                        </LinearLayout>
                    </TableRow>
                </TableLayout>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
</ScrollView>
</LinearLayout>
